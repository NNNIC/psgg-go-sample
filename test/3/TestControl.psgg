###VARIOUS-CONTENTS-BEGIN###
<<<?state/^C_/
eof>>>

/*
    [[state]]
    [[state-cmt]]
*/
<<<?state/^E_/
[[embed]]
eof>>>

<<<?state-typ/^start$/
var [[state>>lc]] = func( bFirst  bool ) {
    gotoState("[[nextstate]]")
}
eof>>>

<<<?state-typ/^end$/
var [[state>>lc]] = func ( bFirst  bool ) {
     // end of state machine
}
eof>>>

<<<?state-typ/^gosub$/
var [[state>>lc]] = func ( bFirst bool ) {
    goSubState("[[gosubstate]]", "[[nextstate]]")
}
eof>>>

<<<?state-typ/^subreturn$/
var [[state>>lc]] = func ( bFirst bool ) {
    returnState()
}
eof>>>

<<<?state-typ/^loop$/
[[members]]
var [[state>>lc]] = func ( bFirst bool ) {
    [[loop_init]]
    gotoState("[[state]]_LoopCheckAndGosub____")
}
var [[state>>lc]]LoopCheckAndGosub = func ( bFirst bool ) {
    if [[loop_cond]] {
        goSubState("[[gosubstate]]", "[[state]]_LoopNext____")
    }
    else {
        gotoState("[[nextstate]]")
    }
}
var [[state>>lc]]LoopNext____ = func(bFirst bool ) {
    [[loop_next]]
    gotoState("[[state]]LoopCheckAndGosub")
}
eof>>>


[[members]]
var [[state>>lc]] = func( bFirst  bool ) {

    [[vars]]

    <<<?init
    if bFirst {
        [[init]]
    }
    >>>

    [[update]]

    <<<?wait
    if [[wait]] {
         return
    }
    >>>

    [[post_wait]]

    [[branch]]

    <<<?nextstate
    if !hasNextState() {
        gotoState("[[nextstate]]")
    }
    >>>
}
###VARIOUS-CONTENTS-END###


